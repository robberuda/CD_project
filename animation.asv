%animation of drone
d = 0.2;



i=1;
for t=1:tss:Tfinal;  
    x = P(i,1);
    y = P(i,2);
    
    d1x = x - cos(P(i,3));
    d1y = y - sin(P(i,3));
    d2x = x + cos(P(i,3));
    d2y = y + cos(P(i,3));
    
    plot([dx1 2],[1 4],P(:,1),P(:,2),x,y,'s','MarkerSize',15,'MarkerFaceColor','R','MarkerEdgeColor','R');
    %plot([1 2],[1 4]);
    %ylim([-5,5]);
    anim(i)=getframe;
    i=i+1;
end

clear i;
    